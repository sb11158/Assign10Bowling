@{
    ViewData["Title"] = "Home Page";
    if (Model.TeamName == null) {
        ViewData["Team"] = "";

    }
    else
    {
        ViewData["Team"] = Model.TeamName;

    }

}

@model IndexViewModel

<h1>@Model.TeamName</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                First Name
            </th>
            <th>
                Middle Intial
            </th>
            <th>
                Last Name
            </th>
            <th>
                Address
            </th>
            <th>
                City
            </th>
            <th>
                State
            </th>
            <th>
                Zipcode
            </th>
            <th>
                Phone Number
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model.Bowlers)
        {
            <tr>
                <td>
                    @x.BowlerFirstName
                </td>
                <td>
                    @x.BowlerMiddleInit
                </td>
                <td>
                    @x.BowlerLastName
                </td>
                <td>
                    @x.BowlerAddress
                </td>
                <td>
                    @x.BowlerCity
                </td>
                <td>
                    @x.BowlerState
                </td>
                <td>
                    @x.BowlerZip
                </td>
                <td>
                    @x.BowlerPhoneNumber
                </td>
            </tr>
        }
    </tbody>
</table>


<div page-info="@Model.PageNumberingInfo"
     page-url-teamname="@Model.TeamName">

</div>
